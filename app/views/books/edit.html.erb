<h1>Update book</h1>
<%= link_to 'back',books_path %>
<%= form_for @book do |f| %>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :author %>
        <%= f.text_field :author %>
    </div>
    <div>
        <%= f.label :review %>
        <%= f.text_field :review %>
    </div>
    <%= f.submit %>
<% end %>